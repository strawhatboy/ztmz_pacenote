{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildRelease",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary",
                "/property:Configuration=Release"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "cleanRelease",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary",
                "/property:Configuration=Release"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildDebug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Run",
            "command": "dotnet",
            "type": "process",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "cwd": "${workspaceFolder}/ZTMZ.PacenoteTool"
            }
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/ZTMZ.PacenoteTool/ZTMZ.PacenoteTool.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "package_normal",
            "command": "ISCC",
            "type": "process",
            "args": [
                "${workspaceFolder}/setup_normal.iss"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "cwd": "${workspaceFolder}"
            }
        },
        {
            "label": "package_dev",
            "command": "ISCC.exe",
            "type": "process",
            "args": [
                "${workspaceFolder}/setup_dev.iss"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "cwd": "${workspaceFolder}"
            }
        }
    ]
}
