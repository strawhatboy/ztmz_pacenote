<Application x:Class="ZTMZ.PacenoteTool.WpfGUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZTMZ.PacenoteTool.WpfGUI"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             DispatcherUnhandledException="OnDispatcherUnhandledException"
            Exit="OnExit"
            Startup="OnStartup"
             >
    <Application.Resources>
         
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Light" />
                <ui:ControlsDictionary />
                <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
                    <FontFamily x:Key="SegoeFluentIcons">pack://application:,,,/ZTMZ.PacenoteTool.Base.UI;component/Res/#Segoe Fluent Icons</FontFamily>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
